{
    "eslint.enable": false,
    "jshint.enable": false,
    "standard.enable": false,
    "workbench.colorCustomizations": {
        "editor.rangeHighlightBackground": "#0f120f",
        "list.activeSelectionForeground": "#0d0d0e",
        "list.inactiveSelectionForeground": "#ffea00",
        "list.highlightForeground": "#121312",
        "scrollbarSlider.activeBackground": "#c8ff00ee",
        "editorSuggestWidget.highlightForeground": "#c8ff0014",
        "textLink.foreground": "#ffff00",
        "progressBar.background": "#C6FF00",
        "pickerGroup.foreground": "#C6FF00",
        "notificationLink.foreground": "#C6FF00",
        "editorWidget.resizeBorder": "#C6FF00",
        "editorWidget.border": "#C6FF00",
        "settings.modifiedItemIndicator": "#C6FF00",
        "settings.headerForeground": "#135564",
        "panelTitle.activeBorder": "#C6FF00",
        "breadcrumb.activeSelectionForeground": "#C6FF00",
        "menu.selectionForeground": "#39ae93",
        "menubar.selectionForeground": "#1b1d15",
        "editor.selectionBackground": "#0e3a44",
        "editor.selectionHighlightBackground": "#1355640b",
        "iLoveWorkSpaceColors": true,
        "iLoveWorkSpaceRandom": false,
        "statusBar.foreground": "#FFE5FA",
        "statusBar.background": "#9F2F88",
        "statusBarItem.hoverBackground": "#732A4B",
        "statusBarItem.remoteForeground": "#FFD3D7",
        "statusBarItem.remoteBackground": "#A43A42",
        "statusBar.debuggingForeground": "#2D0023",
        "statusBar.debuggingBackground": "#D566BE"
    },
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": true
    },
    "explorerExclude.backup": {}
}